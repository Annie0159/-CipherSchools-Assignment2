#include<iostream>


using namespace std;

int main()
{
    int n,i,sum=INT_MAX,j,k,t;
    cin>>n;
    int a[n];
    for(i=0;i<n;i++)
    {
        cin>>a[i];
    }
    cin>>t;
    for(i=0;i<n;i++)
    {
        for(j=i+1;j<n;j++)
        {

            for(k=j+1;k<n;k++)
            {
                if(abs(t-sum)>abs(t-(a[i]+a[j]+a[k])))
                {
                    sum=(a[i]+a[j]+a[k]);
                }
            }
        }
    }
    cout<<sum;
    return 0;
}
